{
	"_version": "1.12.0",
	"sap.app": {
		"id": "calculator.ui",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"calculator": {
				"restApiPost": "../calculator-web-1.0-SNAPSHOT/api/v1/calculate/",
				"restApiGet": "../calculator-web-1.0-SNAPSHOT/api/v1/expressions/",
				"restApiHistoryId": "../calculator-web-1.0-SNAPSHOT/api/v1/history/id",
				"restApiHistory": "../calculator-web-1.0-SNAPSHOT/api/v1/history/"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "calculator.ui.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "calculator.ui.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	}
}